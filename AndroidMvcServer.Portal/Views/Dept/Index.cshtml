@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link href="~/Content/Style/style.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-1.8.2.min.js"></script>
    <script src="~/Scripts/FunctionJS.js"></script>
    <link href="~/Content/Extjs4_2/resources/css/ext-all-neptune.css" rel="stylesheet" />
    <script src="~/Content/Extjs4_2/bootstrap.js"></script>
    <script src="~/Content/Scripts/DeptTreeGrid.js"></script>
</head>
<body>
    <div class="tools_bar">
        <a class="tools_btn" onclick="Replace();" title="刷新当前页面">
            <span>
                <b style="background: url('Content/Themes/images/16/arrow_refresh.png') 50% 4px no-repeat;">刷新</b>
            </span>
        </a>
        <div class="tools_separator"></div>
        <a class="tools_btn" onclick="add();" title="">
            <span class="">
                <b style="background: url('Content/Themes/images/16/application_add.png') 50% 4px no-repeat;">新增</b>
            </span>

        </a>
        <a class="tools_btn" onclick="edit();" title="">
            <span>
                <b style="background: url('Content/Themes/images/16/application_edit.png') 50% 4px no-repeat;">编辑</b>
            </span>

        </a>
        <a class="tools_btn" onclick="Delete();" title="">
            <span>
                <b style="background: url('Content/Themes/images/16/application_delete.png') 50% 4px no-repeat;">删除</b>
            </span>
        </a>
        <div class="tools_separator"></div>
        <a class="tools_btn" onclick="ThisCloseTab();" title="关闭当前窗口"><span class=""><b style="background: url('Content/Themes/images/16/back.png') 50% 4px no-repeat;">离开</b></span></a>
    </div>
</body>
</html>